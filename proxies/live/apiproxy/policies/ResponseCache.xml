<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ResponseCache async="false" continueOnError="false" enabled="true" name="Response-Cache-SDS">
    <DisplayName>Response Cache SDS</DisplayName>
    <Properties/>
    <CacheKey>
        <KeyFragment ref="request.verb"/>
        <KeyFragment ref="request.path"/>
        <KeyFragment ref="request.queryparam.org-code"/>
        <KeyFragment ref="request.queryparam.service-id"/>
    </CacheKey>
    <Scope>Exclusive</Scope>
    <ExpirySettings>
        <ExpiryDate/>
        <TimeOfDay/>
        <TimeoutInSeconds ref="">900</TimeoutInSeconds>
    </ExpirySettings>
    <SkipCacheLookup>(request.header.cache-control = "no-cache") or (request.path  = "/_ping") or (request.path = "/_status") or (request.path  = "/healthcheck")</SkipCacheLookup>
    <SkipCachePopulation>(request.header.cache-control = "no-cache") or (request.path  = "/_ping") or (request.path = "/_status") or (request.path  = "/healthcheck")</SkipCachePopulation>
    <CacheLookupTimeoutInSeconds>30</CacheLookupTimeoutInSeconds>
    <ExcludeErrorResponse>true</ExcludeErrorResponse>
    <UseAcceptHeader>true</UseAcceptHeader>
</ResponseCache>
