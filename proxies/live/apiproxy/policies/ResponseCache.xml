<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ResponseCache async="false" continueOnError="false" enabled="true" name="Response-Cache-SDS">
    <DisplayName>Response Cache SDS</DisplayName>
    <Properties/>
    <CacheKey>
        <KeyFragment ref="request.path"/>
        <KeyFragment ref="request.queryparam.organization"/>
        <KeyFragment ref="request.queryparam.identifier.values"/>
        <KeyFragment ref="request.queryparam.managing-organization"/>
    </CacheKey>
    <Scope>Exclusive</Scope>
    <ExpirySettings>
        <ExpiryDate/>
        <TimeOfDay/>
        <TimeoutInSeconds ref="">900</TimeoutInSeconds>
    </ExpirySettings>
    <SkipCacheLookup>request.header.cache-control = "no-cache"</SkipCacheLookup>
    <SkipCachePopulation/>
    <CacheLookupTimeoutInSeconds>30</CacheLookupTimeoutInSeconds>
    <ExcludeErrorResponse>true</ExcludeErrorResponse>
    <UseAcceptHeader>true</UseAcceptHeader>
</ResponseCache>
